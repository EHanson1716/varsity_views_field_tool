<!-- Simple full-width example-->

<div class="page-header">
  <h1>Photographer #<%= @photographer.id %></h1>
</div>

<div class="row">
  <div class="col-md-12">
    <dl class="dl-horizontal">
      <dt>First name</dt>
      <dd><%= @photographer.first_name %></dd>

      <dt>Last name</dt>
      <dd><%= @photographer.last_name %></dd>

      <dt>Phone</dt>
      <dd><%= @photographer.phone %></dd>

      <dt>Email</dt>
      <dd><%= @photographer.email %></dd>

      <dt>Paypal</dt>
      <dd><%= @photographer.paypal %></dd>

      <dt>String</dt>
      <dd><%= @photographer.string %></dd>

      <dt></dt>
      <dd>
        <div class="btn-group">
          <a href="/photographers" class="btn btn-primary">
            <i class="fa fa-chevron-left"></i>
          </a>

          <a href="/photographers/<%= @photographer.id %>/edit" class="btn btn-warning">
            <i class="fa fa-edit"></i>
          </a>

          <a href="/delete_photographer/<%= @photographer.id %>" class="btn btn-danger">
            <i class="fa fa-trash-o"></i>
          </a>
        </div>
      </dd>
    </dl>

  </div>
</div>

<!-- End simple full-width example-->

<!-- Responsive panel example-->

<div class="row">
  <div class="col-sm-offset-1 col-sm-10 col-md-offset-2 col-md-8">
    <div class="panel panel-default">
      <div class="panel-heading">
        <h3 class="panel-title">Photographer #<%= @photographer.id %></h3>
      </div>
      <div class="panel-body">
        <div class="row">
          <div class="col-md-12">
            <dl class="dl-horizontal">
              <dt>First name</dt>
              <dd><%= @photographer.first_name %></dd>

              <dt>Last name</dt>
              <dd><%= @photographer.last_name %></dd>

              <dt>Phone</dt>
              <dd><%= @photographer.phone %></dd>

              <dt>Email</dt>
              <dd><%= @photographer.email %></dd>

              <dt>Paypal</dt>
              <dd><%= @photographer.paypal %></dd>

              <dt>String</dt>
              <dd><%= @photographer.string %></dd>

            </dl>
            <div class="btn-group btn-group-justified">
              <a href="/photographers" class="btn btn-primary">
                <i class="fa fa-chevron-left"></i>
              </a>

              <a href="/photographers/<%= @photographer.id %>/edit" class="btn btn-warning">
                <i class="fa fa-edit"></i>
              </a>

              <a href="/delete_photographer/<%= @photographer.id %>" class="btn btn-danger">
                <i class="fa fa-trash-o"></i>
              </a>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>

<!-- End responsive panel example -->
